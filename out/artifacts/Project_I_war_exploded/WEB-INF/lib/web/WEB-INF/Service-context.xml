<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:component-scan base-package="com.github.djuloori.whiteboard.service" />
    <bean id = "classservice"       class = "com.github.djuloori.whiteboard.service.ClassService"/>
    <bean id = "userservice"        class = "com.github.djuloori.whiteboard.service.UserService"/>
    <bean id = "taservice"          class = "com.github.djuloori.whiteboard.service.TaService"/>
    <bean id = "syllabusservice"    class = "com.github.djuloori.whiteboard.service.SyllabusService"/>
    <bean id = "scheduleservice"    class = "com.github.djuloori.whiteboard.service.ScheduleService"/>
    <bean id = "assignmentservice"  class = "com.github.djuloori.whiteboard.service.AssignmentService"/>

</beans>